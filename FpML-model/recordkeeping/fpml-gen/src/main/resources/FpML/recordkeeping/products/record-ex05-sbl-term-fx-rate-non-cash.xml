        <type>ISDA2014StandardCreditSupportAgreement</type>
        <date>2019-02-07</date>
      </creditSupportAgreement>
      <otherAgreement>
        <type>GeneralClearingAgreement</type>
        <version>2018</version>
        <date>2019-02-07</date>
      </otherAgreement>
    </documentation>
